%global __os_install_post %{nil}
%define _build_id_links none
%define _binary_payload w19.zstdio
Name:       PACKAGE_NAME
Version:    VERSION
Release:    1
Summary:    PACKAGE_NAME source rpm
BuildArch:  PACKAGE_ARCH
License:    Apache
AutoReq: no

%description
PACKAGE_NAME source rpm for PACKAGE_ARCH

%prep

%build

%install
cp -a ARCHIVE_ROOT/SOURCES/PACKAGE_NAME/* ARCHIVE_ROOT/BUILDROOT/PACKAGE_NAME-%{version}-RELEASE.RPM_BUILD_PROCESSOR/
cd ARCHIVE_ROOT/BUILDROOT/PACKAGE_NAME-%{version}-RELEASE.RPM_BUILD_PROCESSOR/
/usr/bin/chmod -Rf a+rX,u+w,g-w,o-w .

%files

%clean
mv ARCHIVE_ROOT/RPMS/PACKAGE_ARCH/PACKAGE_NAME-%{version}-RELEASE.PACKAGE_ARCH.rpm ARCHIVE_ROOT/RPMS/PACKAGE_ARCH/PACKAGE_NAME.rpm

%changelog
