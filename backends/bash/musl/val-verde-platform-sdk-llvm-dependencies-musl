    ENABLE_STAGE_ROOT_BUILD=TRUE \
    PYTHON_FOR_BUILD=${PYTHON} \
    package-invoke-builder python-cross 3
else
    ENABLE_STAGE_ROOT_BUILD=TRUE \
    _PYTHON_HOST_PLATFORM=${HOST_OS}-${HOST_PROCESSOR} \
    package-invoke-builder python-cross 3
fi

